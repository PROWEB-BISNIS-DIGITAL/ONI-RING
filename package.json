{
  "name": "express-app",
  "version": "1.0.0",
  "description": "A simple Express.js application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "server": "nodemon src/app.js",
    "tailwind": "tailwindcss -i ./public/css/tailwind.css -o ./public/css/output.css --watch",
    "dev": "concurrently \"npm run server\" \"npm run tailwind\""
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^4.17.1",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "nodemon": "^2.0.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  },
  "author": "",
